
        <section class="notice_title">
            <article class="w1440_notice">
                <h1>참여마당</h1>
                <nav>
                    <ul>
                        <li class="notice_active"><a class="noticetxt_active" href="notice">공지사항</a></li>
                        <li><a href="#">행사안내</a></li>
                        <li><a href="#">언론보도</a></li>
                        <li><a href="#">문의하기</a></li>
                        <li class="none_w670" ><a href="#">현장스케치</a></li>
                    </ul>
                </nav>
            </article>
        </section>
        <form method="GET" id="search" action="/search">
        <section class="search_wrap w1440_notice">
            <article>
                <div class="search_bg">
                    <div class="dropbox">
                        <select required name="content" id="">
                            <option value="title">제목</option>
                        </select>
                    </div>
                    <div class="search_bar">
                        <input type="text" name="search_txt" id="searchTxt" placeholder="검색어를 입력하세요.">
                        <input type="submit" class="search_btn" value="">
                    </div>
                </div>
            </article>
        </section>
        </form>
        <form>
        <section class="board_wrap w1440_notice">
            <article>
                <table class="notice_list">
                    <thead>
                        <tr>
                            <td class="notice_tit">제목</td>
                            <td class="date">날짜</td>
                            <td class="file">첨부파일</td>
                        </tr>
                    </thead>
                    <tbody>
                        <% rows.forEach((row, index) => { %>
                        <tr class="paging">
                            <td class="notice_tit" onclick="javascript:location.href='/notice_page?id=<%=row.id%>'"><%=row.title%></td>
                            <td class="date"><%=row.NTdate%></td>
                            <td class="file">
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </article>
        </section>
        </form>
        <section class="pageno_wrap w1440_notice">
            <nav aria-label="Page navigation example">
                <ul class="pagination pagination-seperated notice_num "></ul>
            </nav>
            <div class="write_btn">
                <button type="button" onclick="javascript:location.href='/notice_write'">글쓰기</button>
                <button type="button" onclick="javascript:location.href='/notice'">목록</button>
            </div>
        </section>
