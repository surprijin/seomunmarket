        <section class="notice_title">
            <article class="w1440_notice">
                <h1>참여마당</h1>
                <nav>
                    <ul>
                        <li class="notice_active"><a class="noticetxt_active" href="notice">공지사항</a></li>
                        <li><a href="#">행사안내</a></li>
                        <li><a href="#">언론보도</a></li>
                        <li><a href="#">문의하기</a></li>
                        <li class="none_w670" ><a href="#">현장스케치</a></li>
                    </ul>
                </nav>
            </article>
        </section>
        <section class="search_wrap w1440_notice">
            <article>
                <div class="search_bg">
                    <div class="dropbox">
                        <select required name="content" id="">
                            <option value="title">제목</option>
                            <option value="content">내용</option>
                            <option value="title+content">제목+내용</option>
                        </select>
                    </div>
                    <div class="search_bar">
                        <input type="text" placeholder="검색어를 입력하세요.">
                        <span>
                            <a href="#"></a>
                        </span>
                    </div>
                </div>
            </article>
        </section>
        <section class="board_wrap w1440_notice">
            <article>
                <table class="notice_list">
                    <thead>
                        <tr>
                            <td class="notice_tit">제목</td>
                            <td class="date">날짜</td>
                            <td class="file">첨부파일</td>
                        </tr>
                    </thead>
                    <% rows.forEach((row, index) => { %>
                    <tbody>
                        <tr>
                            <td class="notice_tit" onclick="javascript:location.href='/notice_page?id=<%=row.id%>'"><%=row.title%></td>
                            <td class="date"><%=row.NTdate%></td>
                            <td class="file">
                            </td>
                        </tr>
                    </tbody>
                    <% }) %>
                </table>
            </article>
        </section>
        <section class="pageno_wrap w1440_notice">
            <article>
                <div class="pre_page">
                    <a class="pre" href="#">&lt;&lt;</a>
                    <a class="pre" href="#">&lt;</a>
                </div>
                <div class="page">
                    <a class="active_num" href="#">1</a>
                    <a href="#">2</a>
                    <a href="#">3</a>
                    <a href="#">4</a>
                    <a href="#">5</a>
                    <a class="none_num" href="#">6</a>
                    <a class="none_num" href="#">7</a>
                    <a class="none_num" href="#">8</a>
                    <a class="none_num" href="#">9</a>
                    <a class="none_num" href="#">10</a>
                </div>
                <div class="next_page">
                    <a class="next" href="#">&gt;</a>
                    <a class="next" href="#">&gt;&gt;</a>
                </div>
            </article>
            <div class="write_btn">
                <button type="button" onclick="javascript:location.href='/notice_write'">글쓰기</button>
            </div>
        </section>