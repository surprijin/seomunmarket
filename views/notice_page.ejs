        <section class="nav_wrap">
            <article class="w1440_notipage">
                <h1>참여마당</h1>
                <nav>
                    <ul>
                        <li class="notice_active"><a class="noticetxt_active" href="notice">공지사항</a></li>
                        <li><a href="#">행사안내</a></li>
                        <li><a href="#">언론보도</a></li>
                        <li><a href="#">문의하기</a></li>
                        <li class="none_w670" ><a href="#">현장스케치</a></li>
                    </ul>
                </nav>
            </article>
        </section>
        <section>
            <article class="w1440_notipage view_notice">
                <div class="btn_share">
                    <a href="#"></a>
                </div>
                <% if(typeof errs !== 'undefined'){ %>
                    <% errs.forEach(() =>{ %>
                        <h3><%= err.param %> : <%= err.msg %></h3>
                    <% }) %>
                <%}%>
                <form>
                    <input type="hidden" name="id" value="<%=row.id%>">
                    <table class="notice_view">
                        <colgroup>
                            <col width=" 20%">
                            <col width=" 80%">
                        </colgroup>
                        <thead class="view_thead">
                            <tr>
                                <th colspan="2"><%=row.title%></th>
                            </tr>
                        </thead>
                        <tbody class="view_tbody">
                            <tr class="date">
                                <td class="noti_tit_view">작성일</td>
                                <td class="noti_txt_view"><%=row.NTdate%></td>
                            </tr>
                            <tr class="view_file">
                                <td class="noti_tit_view">첨부파일</td>
                                <td class="noti_txt">
                                    <a class="click" href="#">품평회 심사 대상자 2022.04.pdf</a>
                                    <a class="click" href="#">야시장 품평회 참가자 설명자료 2022.04.pdf</a>
                                </td>
                            </tr>
                            <tr>
                                <td class="content" colspan="2" style="white-space: pre-line;">
                                    <%=row.content%>
                                </td>
                            </tr>
                            <tr>
                                <td class="noti_tit_view">이전글</td>
                                <td class="noti_txt"><a class="click" href="#">이전글제목입니다.</a></td>
                            </tr>
                            <tr>
                                <td class="noti_tit_view">다음글</td>
                                <td class="noti_txt"><a class="click" href="#">다음글 제목입니다</a></td>
                            </tr>
                        </tbody>
                        <tfoot>
                            <tr>
                                <td class="list" colspan="2">
                                    <input type="button" value="목록" onclick = "location.href = 'notice' ">
                                    <input type="button" value="수정" onclick="javascript:location.href='/noticeUpdate?id=<%=row.id%>'" >
                                    <input type="button" value="삭제" onclick="javascript:location.href='/deleteMemo?id=<%=row.id%>'" value="삭제">
                                </td>
                            </tr>
                        </tfoot>
                </table>
                </form>
            </article>
        </section>